    <dataitem name="I010/550" frn="20" format="fixed" length="1">
      <title>System Status</title>
      <definition>Information concerning the configuration and status of a system</definition>
      <structure octet="1" frombit="8" tobit="7">
        <name>NOGO</name>
        <codes>
            <messageMap>
                <entry>
                    <key>0</key>
                    <value>Operational</value>
                </entry>        
                <entry>
                    <key>1</key>
                    <value>Degraded</value>
                </entry>
                <entry>
                    <key>2</key>
                    <value>NOGO</value>
                </entry>
            </messageMap>
        </codes>
      </structure>
      <structure octet="1" frombit="6" tobit="6">
        <name>OVL</name>
        <codes>
            <messageMap>
                <entry>
                    <key>0</key>
                    <value>No overload</value>
                </entry>        
                <entry>
                    <key>1</key>
                    <value>Overload</value>
                </entry>
            </messageMap>
        </codes>
      </structure>
      <structure octet="1" frombit="5" tobit="5">
        <name>TSV</name>
        <codes>
            <messageMap>
                <entry>
                    <key>0</key>
                    <value>valid</value>
                </entry>        
                <entry>
                    <key>1</key>
                    <value>invalid</value>
                </entry>
            </messageMap>
        </codes>
      </structure>
      <structure octet="1" frombit="4" tobit="4">
        <name>DIV</name>
        <codes>
            <messageMap>
                <entry>
                    <key>0</key>
                    <value>Normal Operation</value>
                </entry>        
                <entry>
                    <key>1</key>
                    <value>Diversity degraded</value>
                </entry>
            </messageMap>
        </codes>
      </structure>
      <structure octet="1" frombit="3" tobit="3">
        <name>TTF</name>
        <codes>
            <messageMap>
                <entry>
                    <key>0</key>
                    <value>Test Target Operative</value>
                </entry>        
                <entry>
                    <key>1</key>
                    <value>Test Target Failure</value>
                </entry>
            </messageMap>
        </codes>
      </structure>
      <structure octet="1" frombit="2" tobit="1">
        <name>SPARE</name>
        <mustbe>0</mustbe>
        <format>uint</format>
        <unit></unit>
        <scale>1</scale>
        <desc>Spare bits set to zero</desc>
      </structure>
    </dataitem>  
    
